<!--legends.wxml-->
<view class="container legend-list">
  <block wx:for="{{banners}}" wx:key="id">
    <text class="legend-item">{{item.nickname}}</text>
  <view class="image-section">
   <image style="width: 80px; height: 80px; background-color: #eeeeee;" mode="aspectFit" src="{{item.img}}"></image>
   </view>
  </block>
</view>
